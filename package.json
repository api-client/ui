{
  "name": "@api-client/ui",
  "version": "0.0.0",
  "description": "UI for API Client.",
  "license": "CC-BY-2.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./api-icon.js": "./dist/define/api-icon.js"
  },
  "type": "module",
  "author": {
    "name": "Pawel Uchida-Psztyc",
    "email": "pawel.psztyc@gmail.com"
  },
  "contributors": [
    "Your name can be here!"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/api-components/ui.git"
  },
  "bugs": {
    "url": "https://github.com/api-components/ui/issues",
    "email": "arc@mulesoft.com"
  },
  "scripts": {
    "analyze": "cem analyze --litelement",
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
    "build": "tsc && npm run analyze -- --exclude dist",
    "prepublish": "tsc && npm run analyze -- --exclude dist",
    "lint": "eslint --ext .ts,.html . --ignore-path .gitignore && prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format": "eslint --ext .ts,.html . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "test": "tsc && wtr --coverage --playwright --browsers chromium firefox webkit",
    "test:watch": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch --playwright --browsers chromium\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@anypoint-web-components/awc": "^2.0.9",
    "@api-client/context-menu": "^0.3.2",
    "@api-client/core": "^0.6.0",
    "@api-client/json": "^0.1.3",
    "@github/time-elements": "^3.1.2",
    "@open-wc/dedupe-mixin": "^1.3.0",
    "idb-keyval": "^6.1.0",
    "lit": "^2.2.2",
    "monaco-editor": "^0.33.0",
    "prismjs": "^1.28.0"
  },
  "devDependencies": {
    "@api-client/net-store": "^0.6.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@open-wc/eslint-config": "^7.0.0",
    "@open-wc/testing": "^3.1.3",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@web/dev-server": "^0.1.30",
    "@web/dev-server-esbuild": "^0.2.16",
    "@web/test-runner": "0.13.27",
    "@web/test-runner-playwright": "^0.8.8",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "sinon": "^13.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "customElements": "custom-elements.json",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  }
}
